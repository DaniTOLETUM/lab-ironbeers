<div class="single-beer">
    <div id="image">
        <img class=” beer-img-top” src="{{this.image_url}}" alt=“Beer image cap”>
    </div>
    <div class=“beerBody”>
        <h3 class=“beerName”>{{this.name}}</h3>
        <h4 class=“beerTagline”>{{this.tagline}}</h4>
        <p class=“beerDescription”>{{this.description}}</p>
    </div>
</div>